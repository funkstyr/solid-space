{
  "name": "solid-space",
  "scripts": {
    "_": "---------------------------",
    "dev": "solid-start dev",
    "build": "solid-start build",
    "start": "solid-start start",
    "__": "---------------------------",
    "test": "vitest run",
    "test-watch": "vitest",
    "test-ui": "vitest --ui --coverage.enabled=true",
    "coverage": "vitest run --coverage",
    "coverage:open": "open coverage/index.html",
    "___": "---------------------------",
    "format": "prettier ./src --check",
    "format:fix": "prettier ./src --write",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "typecheck": "tsc --noEmit",
    "____": "---------------------------",
    "deps": "pnpm up --latest -r",
    "_____": "---------------------------",
    "docker:build": "docker build . -t solid-space:latest",
    "docker:run": "docker run -p 3000:3000 solid-space:latest"
  },
  "type": "module",
  "dependencies": {
    "@solidjs/meta": "^0.28.6",
    "@solidjs/router": "^0.8.3",
    "@tanstack/solid-query": "5.0.0-beta.28",
    "@trpc/client": "^10.38.3",
    "@trpc/server": "^10.38.3",
    "class-variance-authority": "^0.7.0",
    "solid-js": "^1.7.11",
    "solid-start": "^0.3.5",
    "solid-start-trpc": "^0.0.16",
    "solid-trpc": "0.1.0-sssr.8",
    "superjson": "^1.13.1",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.4",
    "@testing-library/jest-dom": "^6.1.3",
    "@types/node": "^20.6.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitest/coverage-v8": "^0.34.4",
    "@vitest/ui": "^0.34.4",
    "autoprefixer": "^10.4.15",
    "daisyui": "^3.7.4",
    "eslint": "^8.49.0",
    "eslint-plugin-solid": "^0.13.0",
    "eslint-plugin-testing-library": "^6.0.1",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "solid-start-node": "^0.3.5",
    "tailwindcss": "^3.3.3",
    "turbo": "^1.10.14",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  },
  "engines": {
    "node": ">=16.8"
  },
  "packageManager": "pnpm@8.6.0"
}